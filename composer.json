{
    "name": "clickrain/stratus",
    "description": "Stratus integration for Craft CMS",
    "type": "craft-plugin",
    "keywords": [
      "craft",
      "craftcms",
      "plugin",
      "stratus",
      "clickrain"
    ],
    "license": "proprietary",
    "authors": [
      {
        "name": "Click Rain, Inc.",
        "homepage": "https://clickrain.com"
      }
    ],
    "support": {
      "email": "craft-plugins@clickrain.com"
    },
    "require": {
      "craftcms/cms": "^4.0.0",
      "php": "^8.0.2"
    },
    "autoload": {
      "psr-4": {
        "clickrain\\stratus\\": "src/"
      }
    },
    "extra": {
      "name": "Stratus",
      "handle": "stratus",
      "class": "clickrain\\stratus\\Stratus"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9"
    }
}
